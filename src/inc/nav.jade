.cd-overlay

nav.cd-nav
  
  ul#cd-primary-nav.cd-primary-nav.is-fixed
    each val in nav_major
      if val.sub
        
        li.has-children
          a(href="#{val.url}")= val.page
          
            if val.sub
              ul.cd-nav-gallery.is-hidden
                li.go-back
                  a(href="#0") Main menu
                  
                each subval in val.sub
                  li.cd-nav-item
                    a(href="#{subval.suburl}")
                      img(src="/images/img.jpg", alt="Product Image")
                      h3= subval.subpage
                      
      else
        li
          a(href="#{val.url}")= val.page
  
#cd-search.cd-search
  form
    input(type="search", placeholder="Search...")
